							"pm.test(\"Status code é 200 ou 400 - Cenários válidos\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 400]);",
							"});",
							"",
							"// Validação da mensagem de sucesso para 200",
							"if (pm.response.code === 200) {",

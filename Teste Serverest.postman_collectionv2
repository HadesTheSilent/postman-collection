							"    pm.expect(jsonData.message).to.eql(\"Registro alterado com sucesso\");",
							"});",
							"",
							"// Validação de erro 400 - Campos obrigatórios ausentes",
							"if (pm.response.code === 400) {",
							"    pm.test(\"Status code é 400 - Erro de validação\", function () {",
							"        pm.response.to.have.status(400);",
							"    });",
							"    ",
							"    pm.test(\"Mensagem de erro de validação está presente\", function () {",

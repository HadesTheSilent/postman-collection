							"        var jsonData = pm.response.json();",
							"        pm.expect(jsonData.message).to.be.oneOf([\"Registro excluído com sucesso\", \"Nenhum registro excluído\"]);",
							"    });",
							"    ",
							"    // Limpar variável userId após exclusão bem-sucedida",
							"    if (pm.response.json().message === \"Registro excluído com sucesso\") {",
							"        pm.test(\"Limpar variável userId após exclusão\", function () {",
							"            pm.collectionVariables.unset(\"userId\");",
							"        });",
							"    }",
							"}",
							"",
							"// Validação de erro 400 - Usuário com carrinho ou não encontrado",
							"if (pm.response.code === 400) {",
							"    pm.test(\"Mensagem de erro para regra de negócio\", function () {",
							"        var jsonData = pm.response.json();",
							"        pm.expect(jsonData.message).to.be.oneOf([",
							"            \"Não é permitido excluir usuário com carrinho cadastrado\",",
							"            \"Nenhum registro excluído\"",
							"        ]);",
							"    });",
							"}",
							"",
							"// Validação de erro 401 - Não autorizado",
